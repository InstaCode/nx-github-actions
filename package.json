  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nrwl/cli": "12.3.3",
    "@nrwl/eslint-plugin-nx": "12.3.3",
    "@nrwl/jest": "12.3.3",
    "@nrwl/linter": "12.3.3",
    "@nrwl/tao": "12.3.3",
