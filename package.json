    "eslint-config-prettier": "8.1.0",
    "git-cz": "^4.7.6",
    "husky": "^4.3.5",
    "jest": "26.2.2",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "~9.1.1",
    "typescript": "4.1.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "node tools/hooks/pre-commit.js && lint-staged"
    }
  }
}
